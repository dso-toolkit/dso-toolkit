<a href="{{ url }}"{{#if iconOnly }} title="{{ label }}"{{/if}} {{{ifattr 'class' modifier }}} {{{ifattr 'target' modifier 'extern' '_blank' }}}>
  {{#if (and icon (not iconOnly)) }}{{> '@icon' icon=icon }}{{/if}}<span{{#if iconOnly }} class="sr-only"{{/if}}>{{ label }}</span>{{#if (or iconAfter iconOnly) }}{{> '@icon' icon=(or iconAfter icon) }}{{/if}}
  {{#if badge }}
    {{> '@badge' badge }}
  {{/if}}
</a>
