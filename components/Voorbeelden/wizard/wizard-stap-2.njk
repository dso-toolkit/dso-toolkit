<main>
  <div class="row">
    <div class="col-md-12">
      {% render '@form-steps', {title: steps.title, step: steps.step} %}
      {% render '@search-bar', filter %}
      <div style="width: 100%; height: 300px; background-image: url('https://www.umhs-adolescenthealth.org/wp-content/uploads/2016/12/google-map-background.jpg'); background-size: cover;">
      </div>
      <div class="dso-form-buttons">
        {% render '@button', {type: 'button', modifier: 'default', label: 'Vorige stap', icon: 'chevron-left'} %}
        {% render '@button', {type: 'button', modifier: 'primary', label: 'Volgende stap', iconAfter: 'chevron-right'} %}
      </div>  
  </div>
</main>