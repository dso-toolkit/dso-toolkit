{
  "name": "@dso-toolkit/core",
  "version": "25.0.1",
  "description": "DSO Toolkit Web Components",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/dso-toolkit/dso-toolkit.js",
  "files": [
    "/dist/!(**/*.stories.*)",
    "/loader"
  ],
  "scripts": {
    "clear": "rimraf dist && rimraf loader",
    "build": "stencil build",
    "start": "yarn clear && cross-env DSO_ENV=development yarn node scripts/start.js",
    "test-storybook-static": "start-server-and-test 'http-server --silent --port 56106 www' http://localhost:56106 'cypress run'",
    "watch": "yarn clear && cross-env DSO_ENV=development yarn node scripts/watch.js",
    "cypress": "yarn clear && cross-env DSO_ENV=cypress yarn node scripts/cypress.js",
    "build-www": "cross-env DSO_ENV=production yarn bin_build-storybook --static-dir ./dist --output-dir ./www",
    "bin_stencil": "stencil",
    "bin_start-storybook": "start-storybook",
    "bin_cypress": "cypress",
    "bin_build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@dso-toolkit/sources": "workspace:packages/sources",
    "@popperjs/core": "^2.9.2",
    "@stencil/core": "2.5.2",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "1.4.1",
    "@storybook/addon-a11y": "6.2.9",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addons": "6.2.9",
    "@storybook/web-components": "6.2.9",
    "@types/concurrently": "^6.0.1",
    "@types/debounce": "^1.2.0",
    "@types/sass": "1.16.0",
    "@types/uuid": "^8.3.0",
    "@whitespace/storybook-addon-html": "5.0.0",
    "axe-core": "^4.1.4",
    "babel-loader": "^8.2.2",
    "cheerio": "1.0.0-rc.6",
    "clsx": "1.1.1",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "cypress": "^7.1.0",
    "cypress-axe": "^0.12.2",
    "cypress-real-events": "^1.3.0",
    "debounce": "^1.2.1",
    "http-proxy-middleware": "^1.2.0",
    "http-server": "^0.12.3",
    "lit-html": "^1.3.0",
    "mini-svg-data-uri": "1.2.3",
    "minimist": "^1.2.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.12.5",
    "stencil-inline-svg": "1.0.1",
    "tabbable": "^5.2.0",
    "typescript": "4.2.4",
    "uuid": "^8.3.2",
    "wait-on": "^5.3.0",
    "webpack": "^5.35.0"
  }
}
