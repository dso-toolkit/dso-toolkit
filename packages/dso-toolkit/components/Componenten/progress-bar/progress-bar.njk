<div class="progress">
  <div class="progress-bar" role="progressbar" aria-valuenow="{{ progress }}" aria-valuemin="{{ value.min }}" aria-valuemax="{{ value.max }}" style="width: {{ progress }}%;">
    {%- if progressLabel %}
      <span>{{ progressLabel }}</span>
    {%- else %}
      <span>{{ progress }}%</span>
    {% endif -%}
  </div>
</div>
