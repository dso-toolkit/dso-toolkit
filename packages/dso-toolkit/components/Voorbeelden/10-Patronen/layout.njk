<h1>Voorbeeld grid lay-out</h1>
<section class="grid-example">
  {% for row in rows %}
    <div class="row">
      {% for column in row.columns %}
        <div {{ className(column | prefix('col-') | join(' ')) }}>
          <div class="col">{{ column | join(' ') }}</div>
        </div>
      {% endfor %}
    </div>
  {% endfor %}
</section>
