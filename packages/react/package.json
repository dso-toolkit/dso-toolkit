{
  "name": "@dso-toolkit/react",
  "version": "25.0.1",
  "description": "React specific wrapper for @dso-toolkit/core web components",
  "homepage": "https://www.dso-toolkit.nl",
  "repository": {
    "type": "git",
    "url": "https://github.com/dso-toolkit/dso-toolkit.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "cross-env DSO_ENV=development yarn node scripts/start.js",
    "build": "npm run clean && npm run compile",
    "build-www": "cross-env DSO_ENV=production yarn bin_build-storybook --static-dir ../core/dist,../sources/storybook-assets --output-dir ./www",
    "compile": "tsc --project .",
    "bin_start-storybook": "start-storybook",
    "bin_build-storybook": "build-storybook"
  },
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@dso-toolkit/sources": "workspace:packages/sources",
    "@storybook/addon-a11y": "6.4.22",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addons": "6.4.22",
    "@storybook/builder-webpack5": "6.4.22",
    "@storybook/manager-webpack5": "6.4.22",
    "@storybook/react": "6.4.22",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "3.0.2",
    "storybook-addon-jsx": "^7.3.14",
    "tslib": "2.3.1",
    "typescript": "^4.5.0",
    "wait-on": "^6.0.1",
    "webpack": "^5.72.0"
  },
  "peerDependencies": {
    "@dso-toolkit/core": "^38.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
