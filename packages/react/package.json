{
  "name": "@dso-toolkit/react",
  "version": "25.0.1",
  "description": "React specific wrapper for @dso-toolkit/core web components",
  "homepage": "https://www.dso-toolkit.nl",
  "repository": {
    "type": "git",
    "url": "https://github.com/dso-toolkit/dso-toolkit.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "cross-env DSO_ENV=development yarn node scripts/start.js",
    "build": "npm run clean && npm run compile",
    "build-www": "cross-env DSO_ENV=production yarn bin_build-storybook --static-dir ../core/dist --output-dir ./www",
    "compile": "tsc --project .",
    "bin_start-storybook": "start-storybook",
    "bin_build-storybook": "build-storybook"
  },
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@dso-toolkit/sources": "workspace:packages/sources",
    "@storybook/addon-a11y": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addons": "6.3.12",
    "@storybook/react": "6.3.12",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "3.0.2",
    "storybook-addon-jsx": "^7.3.14",
    "tslib": "2.3.1",
    "typescript": "4.4.4",
    "wait-on": "^6.0.0",
    "webpack": "^5.58.2"
  },
  "peerDependencies": {
    "@dso-toolkit/core": "*",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  }
}
