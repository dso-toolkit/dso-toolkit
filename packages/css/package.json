{
  "name": "@dso-toolkit/css",
  "version": "0.0.0",
  "main": "dist/dso.css",
  "private": true,
  "scripts": {
    "start": "cross-env DSO_ENV=development yarn node scripts/run.js --start",
    "watch": "cross-env DSO_ENV=development yarn node scripts/run.js --watch",
    "build": "rimraf dist && gulp build && sass src/dso.scss dist/dso.css",
    "build-www": "cross-env DSO_ENV=production yarn bin_build-storybook --static-dir ./dist --output-dir ./www",
    "bin_gulp": "gulp",
    "bin_sass": "sass",
    "bin_start-storybook": "start-storybook",
    "bin_build-storybook": "build-storybook"
  },
  "files": [
    "dist",
    "**/*.svg",
    "**/*.scss"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@dso-toolkit/sources": "workspace:packages/sources",
    "@storybook/addon-a11y": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addons": "6.3.12",
    "@storybook/web-components": "6.3.12",
    "@types/css": "^0.0.33",
    "@types/gulp": "^4.0.9",
    "@types/gulp-cheerio": "^0.0.34",
    "@types/gulp-rename": "^2.0.1",
    "@types/gulp-sass": "^5.0.0",
    "@types/gulp-svgmin": "^1.2.1",
    "@types/list-stream": "^1.0.0",
    "@whitespace/storybook-addon-html": "5.0.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "css": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-cheerio": "^1.0.0",
    "gulp-pretty-data": "^0.1.2",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^8.0.0",
    "indent": "^0.0.2",
    "list-stream": "^2.0.0",
    "lit-html": "^2.0.1",
    "minimist": "^1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.43.2",
    "typescript": "4.4.4",
    "wait-on": "^6.0.0",
    "webpack": "^5.58.2"
  }
}
