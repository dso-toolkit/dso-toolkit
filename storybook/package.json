{
  "name": "dso-storybook",
  "private": true,
  "scripts": {
    "start": "storybook dev --port 45000",
    "storybook:build": "storybook build --output-dir ./www",
    "cy:run": "cypress run",
    "e2e": "start-server-and-test 'yarn e2e:start' http-get://127.0.0.1:45000 'yarn e2e:test'",
    "e2e:start": "http-server --silent --port 45000 www",
    "e2e:test": "cypress-parallel --verbose --script cy:run --threads 4 --weights-json cypress-parallel/parallel-weights.json --specsDir cypress/e2e --reporterModulePath ../node_modules/cypress-multi-reporters"
  },
  "dependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@dso-toolkit/core": "workspace:^",
    "@infoprojects/prettier-config": "^1.0.0",
    "@simonsmith/cypress-image-snapshot": "^9.0.3",
    "@storybook/addon-a11y": "7.6.19",
    "@storybook/addon-actions": "7.6.19",
    "@storybook/addon-essentials": "7.6.19",
    "@storybook/addons": "7.6.19",
    "@storybook/api": "^7.6.19",
    "@storybook/components": "^7.6.19",
    "@storybook/core-events": "^7.6.19",
    "@storybook/manager-api": "7.6.19",
    "@storybook/theming": "^7.6.19",
    "@storybook/types": "7.6.19",
    "@storybook/web-components": "7.6.19",
    "@storybook/web-components-webpack5": "7.6.19",
    "@whitespace/storybook-addon-html": "5.1.6",
    "axe-core": "^4.9.1",
    "cypress": "^13.10.0",
    "cypress-axe": "^1.5.0",
    "cypress-multi-reporters": "1.6.4",
    "cypress-parallel": "0.14.0",
    "cypress-real-events": "^1.12.0",
    "dso-toolkit": "workspace:^",
    "http-proxy-middleware": "^3.0.0",
    "http-server": "^14.1.1",
    "iframe-resizer": "^4.4.0",
    "lit": "^3.1.3",
    "lit-html": "^3.1.3",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-syntax-highlighter": "^15.5.0",
    "start-server-and-test": "^2.0.3",
    "storybook": "7.6.19",
    "typescript": "<5.5.0"
  },
  "prettier": "@infoprojects/prettier-config",
  "nx": {
    "includedScripts": []
  }
}
