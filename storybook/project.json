{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build-www": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn storybook:build",
        "cwd": "storybook"
      },
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/www"]
    }
  }
}
